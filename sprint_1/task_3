def isPalindrome(str):
    a = str
    nStr = []
    for i in a:
        if i not in nStr:
            nStr.append(i)
    pal = 0
    for s in nStr:
        num = 0
        for t in str:
            if s == t:
                num += 1
        if num % 2 != 0:
            pal += 1
    if pal < 2:
        return True
    else:
        return False


isPalindrome("23332")

